class Solution 
{
    //Function to reverse words in a given string.
    String reverseWords(String S)
    {
        // code here 
        String a[]=S.split("\\.");
        StringBuilder sb=new StringBuilder();
        
        for(int i=a.length-1;i>=0;i--)
        {
            sb.append(a[i]);
              if (i != 0) {
                sb.append(".");
            }
        }
        
        return sb.toString();
    }
}
