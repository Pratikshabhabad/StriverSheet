//User function Template for Java

class Solution {
    int isPalindrome(String S) {
        // code here
        
        int st=0;
        int end=S.length()-1;
        char arr[]=S.toCharArray();
        
        while(st<end)
        {
            if(arr[st]!=arr[end])
            {
                return 0;
            }
            st++;
            end--;
        }
        
        return 1;
    }
};
